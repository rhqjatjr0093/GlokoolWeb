ate : new Date(),  //가입한 날짜\r\n        birthDate : new Date()\r\n    })\r\n});\r\n\r\nexport default handleActions({\r\n    [CHANGE_INPUT]: (state, action) => {\r\n        const { form, name, value } = action.payload;\r\n        return state.setIn([form, 'form', name], value);\r\n    },\r\n    [INITIALIZE_FORM]: (state, action) => {\r\n        const initialForm = initialState.get(action.payload);\r\n        return state.set(action.payload, initialForm);\r\n    },\r\n    [SET_ERROR]: (state, action) => {\r\n        const { form, message } = action.payload;\r\n        return state.setIn([form, 'error'], message);\r\n    }\r\n}, initialState);","C:\\Work\\GlokoolWeb\\src\\pages\\Auth.js",[],"C:\\Work\\GlokoolWeb\\src\\pages\\Main.js",[],"C:\\Work\\GlokoolWeb\\src\\pages\\Email.js",[],"C:\\Work\\GlokoolWeb\\src\\components\\Base\\LoginHeaderContainer.js",[],"C:\\Work\\GlokoolWeb\\src\\containers\\Main\\Chat.js",["231","232","233","234","235","236","237","238","239","240"],"C:\\Work\\GlokoolWeb\\src\\containers\\Main\\Setting.js",["241","242","243","244","245","246","247","248","249","250","251","252","253","254","255","256","257","258"],"import React, { Component, useState } from \"react\";\r\nimport { auth, storage, firestore } from \"../../Firebase\";\r\nimport { makeStyles } from \"@material-ui/core/styles\";\r\nimport Button from \"@material-ui/core/Button\";\r\nimport TextField from \"@material-ui/core/TextField\";\r\nimport MenuItem from \"@material-ui/core/MenuItem\";\r\nimport Select from \"@material-ui/core/Select\";\r\nimport FormControl from \"@material-ui/core/FormControl\";\r\nimport InputLabel from \"@material-ui/core/InputLabel\";\r\nimport {\r\n  SettingContent,\r\n  InputWithLabel,\r\n  SettingButton,\r\n  SettingDate,\r\n  SettingGender,\r\n  SettingError,\r\n  SettingWrapper,\r\n} from \"../../components/Setting\";\r\nimport profile from \"../../assets/profile.jpg\";\r\nimport ImageUploader from \"react-images-upload\";\r\nimport Modal from \"@materia